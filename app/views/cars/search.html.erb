<div class="ui compact form">
  <%= form_tag(cars_search_path, method: "get") do %>
    <div class="fields">
      <div class="field">
        <%= label_tag(:q, "Search by:") %>
          <%= select_tag "year_id", options_from_collection_for_select(Year.all, "id", "year"), class: "ui search dropdown" %>
        <%= text_field_tag(:q) %>
        <%= submit_tag("Search") %>
      </div>
    </div>
  <% end %>
</div>

<div class="ui cards fluid container">
  <%= render @found_items %>
</div>

